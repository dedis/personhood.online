<!--
Here the user can enter his alias
-->
<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <StackLayout class="p-20">
        <Label text="Setting your alias" class="h1 text-center"/>
        <TextField class="input"></TextField>

        <Button text="Continue" tap="goActivate" class="btn btn-primary btn-active"/>
    </StackLayout>
</Page>
